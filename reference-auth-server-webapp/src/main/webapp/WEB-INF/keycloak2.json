{
  "realm" : "dev",
  "resource" : "reference-auth",
  "auth-server-url" : "https://id.logicahealth.org/auth",
  "ssl-required" : "all",
  "use-resource-role-mappings" : true,
  "enable-cors" : true,
  "cors-max-age" : 1000,
  "cors-allowed-methods" : "POST, PUT, DELETE, GET, OPTIONs",
  "cors-exposed-headers" : "X-FHIR-Starter,authorization,Prefer,Origin,Accept,X-Requested-With,Content-Type,Access-Control-Request-Method,Access-Control-Request-Headers",
  "bearer-only" : true,
  "autodetect-bearer-only" : true,
  "enable-basic-auth" : true,
  "expose-token" : true,
  "verify-token-audience" : true,
  "credentials" : {
    "secret" : "234234-234234-234234"
  },

  "connection-pool-size" : 20,
  "disable-trust-manager": false,
  "allow-any-hostname" : false,
  "truststore" : "classpath://keycloak/truststore.jks",
  "truststore-password" : "changeit",
  "token-store" : "cookie",
  "principal-attribute": "sub, preferred_username, email, name",
  "token-minimum-time-to-live" : 250,
  "verify-token-audience" : "true"
  }
}