[
  {
    "name": "interopio-auth-server",
    "image": "nexus.interopion.com:18083/interopio/interopio-auth-server:{{PROJECT_VERSION}}",
    "cpu": 0,
    "portMappings": [
      {
        "containerPort": 10200,
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "memory": 600,
    "memoryReservation": 400,
    "essential": true,
    "secrets": [
      {
        "name": "JASYPT_ENCRYPTOR_PASSWORD",
        "valueFrom": "arn:aws:secretsmanager:us-west-2:888922156537:secret:interopio/prod/master_encryption_key-ujRe0K"
      }
    ],
    "environment": [
      {
        "name": "SPRING_PROFILES_ACTIVE",
        "value": "prod,mysql,users-interopio,analytics"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/interopio/prod/interopio-auth",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]

