[
  {
    "name": "auth-stage",
    "image": "hspconsortium/auth:{{PROJECT_VERSION}}",
    "cpu": 0,
    "portMappings": [
      {
        "containerPort": 8060,
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "memoryReservation": 512,
    "essential": true,
    "secrets": [
      {
        "name": "JASYPT_ENCRYPTOR_PASSWORD",
        "valueFrom": "arn:aws:secretsmanager:us-west-2:657600230790:secret:hspconsortium/stage/master-encryption-key-iF6zVd"
      }
    ],
    "environment": [
      {
        "name": "SPRING_PROFILES_ACTIVE",
        "value": "stage,users-firebase"
      }
    ]
  }
]
